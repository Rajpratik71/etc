"3.8.1 (created: 2013/11/21 23:49:14)

let mapleader='s'

" common
nmap <Leader>j :tabnext<Return>
nmap <Leader>k :tabprev<Return>

" for osx
nmap <M-j> :tabnext<Return>
nmap <M-k> :tabprev<Return>

" for linux
nmap <C-j> :tabnext<Return>
nmap <C-k> :tabprev<Return>

nmap <Leader>c :emenu Tools.Clearly<Return>
nmap <Leader>d :tab downloads<Return>
nmap <Leader>n :emenu Tools.Web Developer.Network<Return>
nmap <Leader>f :set fullscreen!<Return>
nmap <Leader>p :preferences<Return>
nmap <Leader>s :source! ~/.vimperatorrc<Return>

nmap <BS> :back<Return>

cmap <C-j> <Down>
cmap <C-k> <Up>
cmap <C-h> <Left>
cmap <C-l> <Right>

set defsearch=g

" press <C-]> to make complete
iabbrev mail wolfwzr.china@gmail.com

source! ~wolfwzr/.vimperatorrc.local

" vim: set ft=vim:
